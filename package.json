{
  "name": "x-to-zod",
  "version": "0.4.0",
  "description": "Enhanced fork of json-schema-to-zod - Converts JSON Schema into Zod schemas with fluent builder pattern",
  "types": "./dist/types/index.d.ts",
  "bin": "./dist/cjs/cli.js",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "import": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "require": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/cjs/index.js"
    },
    "*": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "builders": {
      "types": "./dist/types/ZodBuilder/index.d.ts",
      "import": "./dist/esm/ZodBuilder/index.js",
      "require": "./dist/cjs/ZodBuilder/index.js"
    },
    "parsers/json": {
      "types": "./dist/types/JsonSchema/index.d.ts",
      "import": "./dist/esm/JsonSchema/index.js",
      "require": "./dist/cjs/JsonSchema/index.js"
    }
  },
  "scripts": {
    "build:types": "tsc -p tsconfig.types.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && node postcjs.js",
    "build:esm": "tsc -p tsconfig.esm.json && node postesm.js",
    "build": "pnpm test && rimraf ./dist && pnpm run build:types && pnpm run build:cjs && pnpm run build:esm",
    "dry": "pnpm run build && pnpm publish --dry-run",
    "dev": "vitest",
    "test": "vitest run",
    "lint": "oxlint src test",
    "lint:fix": "oxlint --fix src test",
    "format": "oxfmt src test",
    "format:check": "oxfmt --check src test",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm run build && changeset publish"
  },
  "c8": {
    "exclude": [
      "dist",
      "createIndex.ts",
      "jest.config.js",
      "postcjs.js",
      "postesm.js",
      "test"
    ]
  },
  "keywords": [
    "zod",
    "json",
    "schema",
    "converter",
    "cli"
  ],
  "author": "Pradeep Mouli (https://github.com/pradeepmouli)",
  "contributors": [
    "Stefan Terdell (Original Author)",
    "Chen (https://github.com/werifu)",
    "Nuno Carduso (https://github.com/ncardoso-barracuda)",
    "Lars Strojny (https://github.com/lstrojny)",
    "Navtoj Chahal (https://github.com/navtoj)",
    "Ben McCann (https://github.com/benmccann)",
    "Dmitry Zakharov (https://github.com/DZakh)",
    "Michel Turpin (https://github.com/grimly)",
    "David Barratt (https://github.com/davidbarratt)",
    "pevisscher (https://github.com/pevisscher)",
    "Aidin Abedi (https://github.com/aidinabedi)",
    "Brett Zamir (https://github.com/brettz9)",
    "vForgeOne (https://github.com/vforgeone)",
    "Adrian Ordonez (https://github.com/adrianord)",
    "Jonas Reucher (https://github.com/Mantls)"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/pradeepmouli/json-schema-to-zod"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "fast-diff": "^1.3.0",
    "oxfmt": "^0.20.0",
    "oxlint": "^1.35.0",
    "rimraf": "^6.1.2",
    "ts-morph": "^27.0.2",
    "tsx": "^4.21.0",
    "type-fest": "^5.3.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "zod": "^4.2.1"
  },
  "dependencies": {
    "json-schema-typed": "^8.0.2"
  }
}
